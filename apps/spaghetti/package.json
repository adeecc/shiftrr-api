{
  "name": "@shiftrr/spaghetti",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "watch:ts": "tsc -w",
    "clean": "rimraf build",
    "build": "rimraf build && tsc",
    "start:dev": "nodemon build/src/server.js",
    "dev": "rimraf build && tsc && npm-run-all --parallel watch:ts start:dev",
    "start": "node build/src/server.js",
    "format": "yarn prettier --write --config .prettierrc ./*.json ./*.md  ./*.js ./test/*.ts ./src/*.ts ./src/**/*.ts ./src/**/*.json ./.github/* ./.github/**/*",
    "lint": "eslint \"**/*.ts\" --quiet --fix"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "connect-mongo": "^4.6.0",
    "cors": "^2.8.5",
    "debug": "^4.3.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "helmet": "^5.0.2",
    "mongodb": "^4.3.1",
    "mongoose": "^6.2.1",
    "morgan": "^1.10.0",
    "passport": "^0.5.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.4.0",
    "@types/mocha": "^9.1.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.18",
    "@types/passport": "^1.0.7",
    "chai": "^4.3.6",
    "eslint": "^8.9.0",
    "jest": "^27.5.1",
    "mocha": "^9.2.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  }
}
